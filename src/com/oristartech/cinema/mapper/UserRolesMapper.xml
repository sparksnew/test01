<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.oristartech.cinema.mapper.UserRolesMapper">
	<select id="queryRoleByMobile" resultType="com.oristartech.cinema.pojo.UserRoles">
		select roleID from users where mobile=#{mobile} and status = '1'
	</select>
</mapper>